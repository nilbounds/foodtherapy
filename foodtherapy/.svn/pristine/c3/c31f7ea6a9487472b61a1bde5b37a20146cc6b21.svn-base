<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.iiijiaban.foodtherapy"
    android:versionCode="1"
    android:versionName="1.0" >
<uses-permission android:name="android.permission.INTERNET" />
    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity android:name="iijiaban.foodtherapy.ui.materialnavigation.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
             <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
                </intent-filter>
        </activity>
         <activity android:name="iijiaban.foodtherapy.ui.materialnavigation.SearchActivity"></activity>
        <activity android:name="ijiaban.foodtherapy.fragment.NewsDetail"></activity>
        <activity android:name="iijiaban.foodtherapy.ui.materialnavigation.FoodListActivity"></activity>
        <activity android:name="iijiaban.foodtherapy.ui.materialnavigation.FoodDetailActivity"></activity>
        <activity android:name="iijiaban.foodtherapy.ui.materialnavigation.CookDetailActivity"></activity>
        <activity android:name="iijiaban.foodtherapy.ui.materialnavigation.NewsDetailActivity"></activity>
    </application>

</manifest>
